@model ObligT1.Models.EnGangsKode
@{
    ViewBag.Title = "EnGangsKode";
}

<hr />
@using (Html.BeginForm("EnGangsKode", "Kundes"))
{ 
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="row">
            @Html.LabelFor(model => model.Kode, htmlAttributes: new { @class = "col-md-3" })
            <div class="col-md-">
                @Html.EditorFor(model => model.Kode, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Kode, "", new { @class = "text-danger" })
            </div>
            <div class="col-md-2">
                <input type="submit" value="Neste" class="btn btn-default" />
            </div>
        </div>
    </div>
}
